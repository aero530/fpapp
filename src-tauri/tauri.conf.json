{
  "package": {
    "productName": "fpapp",
    "version": "3.0.0"
  },
  "build": {
    "distDir": "../public",
    "devPath": "http://localhost:8080",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.my.fpapp",
      "icon": [
        "../resources/icons/32x32.png",
        "../resources/icons/128x128.png",
        "../resources/icons/256x256.png",
        "../resources/icon.icns",
        "../resources/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "Finance",
      "shortDescription": "Financial planning",
      "longDescription": "Financial planning application",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "dialog": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "fpapp",
        "width": 1600,
        "height": 1200,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}